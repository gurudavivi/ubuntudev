{
  "name": "guru-setup",
  "description": "guru-setup",
  "version": "0.0.1",
  "author": "vivianeflorido @vivianeflowt",
  "license": "MIT",
  "repository": "gurudavivi/guru-fp.git",
  "homepage": "https://github.com/gurudavivi/guru-fp#readme",
  "bugs": "https://github.com/gurudavivi/guru-fp/issues",
  "bin": {
    "guru-setup": "./build/cli.js"
  },
  "main": "dist/cli.js",
  "scripts": {
    "build": "rimraf dist && npm run build:cli",
    "build:cli": "esbuild ./src/index.ts --bundle --platform=node --target=node10 --minify --loader:.ts=ts --outfile=./build/cli.js",
    "build:main": "tsc -p tsconfig.build.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "pnpm run fix:format && pnpm run fix:lint",
    "fix:format": "prettier --write ./src/**/*.{js,mjs,cjs,ts}",
    "fix:lint": "eslint src/**/*.{js,mjs,cjs,ts} --fix",
    "fix:pack": "syncpack format",
    "prebuild": "rimraf dist",
    "typedoc": "./scripts/typedoc.sh",
    "release": "release-it --dry-run --verbose",
    "reset": "./scripts/reset.sh",
    "test": "jest --no-cache --runInBand --logHeapUsage",
    "test:clear": "jest --clearCache",
    "test:cov": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watch",
    "ts": "node -r esbuild-register",
    "typesync": "typesync"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "ansi-escapes": "^5.0.0",
    "boxen": "^6.2.1",
    "chalk": "4",
    "cli-columns": "^4.0.0",
    "cli-truncate": "^3.1.0",
    "commander": "^9.2.0",
    "execa": "^6.1.0",
    "figures": "^4.0.1",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "ora": "^6.1.0",
    "pretty-time": "^1.1.0",
    "string-width": "^5.1.2",
    "strip-ansi": "^7.0.1",
    "widest-line": "^4.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@types/eslint": "^8.4.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/esm": "^3.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/mustache": "^4.1.2",
    "@types/node": "^17.0.24",
    "@types/prettier": "^2.6.0",
    "@types/pretty-time": "^1.1.2",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "benny": "^3.7.1",
    "esbuild": "^0.14.36",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "esm": "^3.2.25",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "release-it": "^15.0.0",
    "rimraf": "^3.0.2",
    "syncpack": "^6.2.1",
    "ts-essentials": "^9.1.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.3.1",
    "type-fest": "^2.12.2",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.6.3",
    "typesync": "^0.9.0",
    "utility-types": "^3.10.0"
  }
}